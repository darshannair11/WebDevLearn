<%- include('header') %>
<body>
    <div class="box" id="heading">
        <h1><%= listType %> !</h1>
    </div>
    <div class="box">
            <% for(var i=0;i<newItems.length;i++){ %>
        <div class="item">
                <input type="checkbox">
                <p><%= newItems[i] %></p>
        </div>
            <%}%> 
    <form action="/" method="post" class="item">
        <input type="text" name="todo" placeholder="New Task" class="input" autocomplete="off">
        <button type="submit" name="list" class="button" value=<%= listType %>>+</button>
    </form>
    </div>
    
    
</body>
<%- include('footer') %>
</html>